<div>
  <% @books.each do |book| %>
  <div>
    <ul>
      <%= image_tag book.image if book.image? %>
      <li><%= book.title %></li>
      <li><%= book.author %></li>
      <li><%= book.page_count %></li>
      <li><%= book.category %></li>
    </ul>
    <% if book.user_id === current_user.id %>
      <%= button_to "本棚から削除", book, method: :delete, data: { confirm: "本当に削除しますか？"} %>
    <% end %>
  </div>
  <% end %>
</div>