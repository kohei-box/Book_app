<h4><%= @title %></h3>
<% @read_books.each do |book| %>
<div>
    <ul>
      <%= image_tag book.image if book.present? %>
      <li><%= book.title %></li>
      <li><%= book.author %></li>
      <li><%= book.page_count %></li>
      <% if @user === current_user %>
        <%= link_to "本棚から削除", book_registration_path(id: book.id), method: :delete, data: { confirm: "本当に削除しますか？"} %>
      <% end %>
    </ul>
</div>
<% end %>